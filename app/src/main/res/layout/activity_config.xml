<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="br.ufpb.dcx.sisalfa.activities.ConfigActivity">

    <Button
        android:id="@+id/impTemas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Importar Temas " />

    <Button
        android:id="@+id/cadastrarDesafios"
        android:layout_toEndOf="@id/impTemas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastrar Desafios" />

</RelativeLayout>
